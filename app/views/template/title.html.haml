.page_for_pdf{:style => "width:794px;background: blue;color: white;height:1135px;"}
  .col-xs-12{:style => "padding:25% 25% 0 25%;"}
    %img{:src => "data:image/png;base64,",width: '100%'}/
  .col-xs-12{:style => "padding:10%"}
    #title_page_title{:style => "font-size: 60px;text-align:center;"}
      = t('.title')
    #title_page_city{:style => 'padding-top: 20px;font-size: 40px;text-align:center;'}
      -if user_signed_in?
        = current_user.town_model.title
      -else
        = t('.city')
    #title_page_year{:style => 'padding-top: 20px;font-size: 30px;text-align:center;'}
      =Time.now.year