    text-align: center;
    font-size: 1.2em;
    font-weight: bold;
    color: rgb(1, 153, 218);
    line-height: 1em;
    padding: 0 5px 5px 5px !important;
    margin: 0;
  }

  .img-town {
    padding: 0 0 10px 0;
  }

  .links, .desc-ons {
    padding: 0 0 5px 0;
    border-bottom: 1px solid rgb(1, 153, 218);
  }

  .bar > li {
